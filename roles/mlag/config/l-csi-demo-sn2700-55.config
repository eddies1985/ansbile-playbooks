

no logging monitor events
no cli session prefix-modes enable
lacp
lldp
ip l3
ip routing
spanning-tree
spanning-tree mode rpvst


protocol mlag
protocol magp
dcb priority-flow-control enable force


# ipl vlan

vlan 4094
interface port-channel  1

# ipl lag

interface ethernet 1/3 channel-group 1 mode active
interface ethernet 1/4 channel-group 1 mode active
interface port-channel 1 dcb priority-flow-control mode on force
interface vlan 4094 ip address 192.168.1.5/30


# mlag

interface port-channel 1 ipl 1

interface vlan 4094 ipl 1 peer-address 192.168.1.6
mlag-vip spine ip 192.168.255.2 /24 force
mlag system-mac 00:00:5E:00:01:02
no mlag shutdown

# interface mlag-port-channel

interface mlag-port-channel 1
interface ethernet 1/1 mlag-channel-group 1 mode active
interface ethernet 1/2 mlag-channel-group 1 mode active
interface mlag-port-channel 1 description leaf
interface mlag-port-channel 1 mtu 9216 force
interface mlag-port-channel 1 no shutdown


# vlans

vlan 1

interface mlag-port-channel 1 switchport mode access
